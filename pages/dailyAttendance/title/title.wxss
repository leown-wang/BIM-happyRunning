/* pages/dailyAttendance/title/title.wxss */
text {
    display: block;
}
.my-level {
    width: 750rpx;
    height: 213rpx;
    margin-bottom: 30rpx;
    background-image: url(data:image/png;base64,);
}
.mylevel-flex {
    display: flex;
    justify-content: space-between;
    padding-top: 25rpx;
    padding-left: 28rpx;
}
.mylevel-icon {
    width: 143rpx;
    height: 104rpx;
}
.mylevel-box {
    flex: 1;
    padding-left: 35rpx;
    color: white;
}
.level-content {
    font-size: 30rpx;
    font-weight: 400;
    margin-bottom: 24rpx;
}
.level-integral {
    width: 205rpx;
    height: 34rpx;
    border-radius: 17rpx;
    background-color: rgba(0, 0, 0, 0.1);
    text-align: center;
    line-height: 34rpx;
    font-size: 24rpx;
    letter-spacing: 1rpx;
}
.level-strategy {
    margin-top: 5rpx;
    margin-right: -10rpx;
    width: 188rpx;
    height: 40rpx;
    border-radius: 20rpx;
    background-color: rgba(87, 211, 230, 1);
    text-align: center;
    font-size: 24rpx;
    color: white;
}
.mylevel-flex navigator {
    background-color: transparent;
}
.strategy-icon {
    width: 20rpx;
    height: 26rpx;
    margin-top: 7rpx;
}

/*--------等级积分模块--------*/
/*三列flex布局*/
.level-list {
    display: flex;
    flex-flow: column;
    justify-content: space-between;    
    width: 670rpx;
    margin: 0 auto;
}
/*行flex布局*/
.row {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}
/*每个奖牌盒子样式*/
.level-box {
    width: 158rpx;
    height: 205rpx;
    margin-bottom: 3.5vh;
}
.level-icon {
    width: 158rpx;
    height: 120rpx;
}
.level-name {
    font-size: 28rpx;
    text-align: center;
    font-weight: 350;
}
.level-require {
    font-size: 24rpx;
    color: rgba(199, 204, 210, 1);
    text-align: center;
}
[src*="1.png"] {
    width: 122rpx;
}